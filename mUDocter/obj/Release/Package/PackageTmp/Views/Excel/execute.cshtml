
<div id="page-wrapper">
    <h2>Tài khoản gặp lỗi đăng ký </h2> <a href="/Excel" class="btn btn-info pull-right">Trở lại</a>
    <div>
        <div class="alert">
            Do email/fb đã được đăng ký
        </div>

        @if (ViewBag.listFailed != null)
        {
            int i = 0;
            <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                <thead>
                    <tr>
                        <th>STT</th>
                        <th>Họ và tên</th>
                        <th>Gmail/fbid</th>
                        <th>Địa chỉ</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var _x in ViewBag.listFailed)
                    {
                        i++;
                        <tr>
                            <td>@i</td>
                            <td>@_x.name</td>
                            <td>@_x.email</td>
                            <td>@_x.address</td>

                        </tr>
                    }
                </tbody>
            </table>
        }
    </div>
    <div class="alert">
        Mật khẩu mặc định của các tài khoản đăng ký thành công là : 123
    </div>

</div>